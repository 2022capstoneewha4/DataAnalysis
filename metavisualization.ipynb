import pandas as pd
import matplotlib.pyplot as plt

csv_meta = pd.read_csv("/content/drive/MyDrive/2022-코딩 과제들/sequences.csv")

csv_meta.head()

csv_meta['Pangolin'].head()
Pangolin_data = pd.DataFrame(csv_meta.groupby('Pangolin').size())
Pangolin_data.head()

